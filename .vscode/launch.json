{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch Consumer",
            "type": "coreclr",
            "request": "launch",                        
            "program": "${workspaceFolder}/SQSConsumer/bin/Debug/netcoreapp3.1/SQSConsumer.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "preLaunchTask": "build",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch Producer",
            "type": "coreclr",
            "request": "launch",                        
            "program": "${workspaceFolder}/SQSProducer/bin/Debug/netcoreapp3.1/SQSProducer.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "preLaunchTask": "build",
            "stopAtEntry": false
        },        
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}